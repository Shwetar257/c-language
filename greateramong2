#include <stdio.h>
int main()
{
    int num1,num2,num3;
    printf("enter a number 1=");
    scanf("%d",&num1);
    printf("enter number 2=");
    scanf("%d", &num2);
    if(num1>num2)
    {
        printf("number1=%d is greater",num1);
    }
    else
    {
        printf("number=%d is greater",num2);
    }
    
    return 0;
}
